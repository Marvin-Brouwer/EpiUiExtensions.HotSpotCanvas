			"usedInLocal": false
		},
		"pathJoin": {
			"code": "export function pathJoin(parts: Array<string>, separator: string = '/') {\n  var replace = new RegExp(separator + '{1,}', 'g');\n  return ('/' + parts.join(separator)).replace(replace, separator);\n}",
			"isFunction": true,
			"usedInLocal": false
		},
		"urlJoin": {
			"code": "export function urlJoin(origin: string, parts: Array<string>) {\n  const path = pathJoin(parts);\n  const url = new URL(origin);\n  url.pathname = path;\n  return url;\n}",
			"isFunction": true,
			"usedInLocal": false
		}
